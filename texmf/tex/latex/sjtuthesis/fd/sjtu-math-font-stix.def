\ProvidesFile{sjtu-math-font-stix.def}
  [2020/07/31 1.0.0rc7 STIX math font definition (SJTUThesis)]
\ifsjtu@unimath
  \setmathfont{STIX2Math}[
    Extension    = .otf,
    StylisticSet = 8,
  ]
  \setmathfont{STIX2Math}[
    Extension    = .otf,
    StylisticSet = 1,
    range        = {scr,bfscr},
  ]
  \setmathrm[
    Extension      = .otf,
    UprightFont    = *-Regular,
    BoldFont       = *-Bold,
    ItalicFont     = *-Italic,
    BoldItalicFont = *-BoldItalic,
  ]{STIX2Text}
  \setmathsf[
    Extension      = .otf,
    UprightFont    = *-Regular ,
    ItalicFont     = *-RegularIt ,
    BoldFont       = *-Bold ,
    BoldItalicFont = *-BoldIt,
  ]{SourceSansPro}
  \setmathtt[
    Scale          = MatchLowercase,
    Extension      = .otf,
    UprightFont    = *-Regular ,
    ItalicFont     = *-RegularIt ,
    BoldFont       = *-Bold ,
    BoldItalicFont = *-BoldIt,
  ]{SourceCodePro}
\else
  \RequirePackage{amssymb}
  \RequirePackage{upgreek}
  \let\oldencodingdefault\encodingdefault
  \let\oldrmdefault\rmdefault
  \let\oldsfdefault\sfdefault
  \let\oldttdefault\ttdefault
  \RequirePackage[T1]{fontenc}
  \RequirePackage[notext,notextcomp,upint]{stix}
  \let\encodingdefault\oldencodingdefault
  \let\rmdefault\oldrmdefault
  \let\sfdefault\oldsfdefault
  \let\ttdefault\oldttdefault
  \RequirePackage{bm}
\fi
\endinput

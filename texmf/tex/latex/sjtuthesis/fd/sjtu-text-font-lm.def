    ItalicFont     = *-oblique,
    BoldItalicFont = *-boldoblique,
  ]{lmsans10}
  \setmonofont[
    Extension      = .otf,
    UprightFont    = *-regular,
    BoldFont       = *-bold,
    ItalicFont     = *-oblique,
    BoldItalicFont = *-boldoblique,
  ]{lmmonolt10}
